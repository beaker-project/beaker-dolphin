{
  "name": "beaker-dolphin",
  "version": "1.0.0",
  "description": "Beaker Workflow God",
  "author": "Martin Styk <mastyk@redhat.com>",
  "license": "GPL-3.0",
  "repository": "https://github.com//beaker-dolphin.git",
  "homepage": "https://github.com//beaker-dolphin",
  "bugs": "https://github.com//beaker-dolphin/issues",
  "keywords": [
    "github",
    "bot-app",
    "dolphin",
    "beaker"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "probot": "^9.11.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "nock": "^12.0.3",
    "nodemon": "^2.0.2",
    "smee-client": "^1.1.0",
    "standard": "^14.3.3"
  },
  "engines": {
    "node": ">= 13.11.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
